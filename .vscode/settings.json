{
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript"
    ],
    "javascript.suggestionActions.enabled": false,
    "typescript.suggestionActions.enabled": false,
    "editor.tabSize": 2,
    "editor.detectIndentation": false
}